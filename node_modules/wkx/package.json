{
  "name": "wkx",
  "version": "0.4.6",
  "description": "A WKT/WKB/EWKT/EWKB/TWKB/GeoJSON parser and serializer",
  "main": "lib/wkx.js",
  "types": "lib/wkx.d.ts",
  "files": [
    "dist/",
    "lib/"
  ],
  "scripts": {
    "test": "jshint . && mocha",
    "build": "mkdirp ./dist && browserify -r buffer -r ./lib/wkx.js:wkx ./lib/wkx.js > ./dist/wkx.js && uglifyjs -c -m -- ./dist/wkx.js > ./dist/wkx.min.js",
    "coveralls": "istanbul cover node_modules/mocha/bin/_mocha -- -R spec && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage"
  },
  "author": "Christian Schwarz",
  "license": "MIT",
  "devDependencies": {
    "async": "^2.6.1",
    "browserify": "^16.2.3",
    "coveralls": "^3.0.2",
    "deep-eql": "^4.0.0",
    "istanbul": "^0.4.5",
    "jshint": "^2.9.7",
    "json-stringify-pretty-compact": "^1.2.0",
    "mkdirp": "^0.5.1",
    "mocha": "^5.2.0",
    "pg": "^7.7.1",
    "uglify-js": "^3.4.9"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/cschwarz/wkx.git"
  },
  "keywords": [
    "wkt",
    "wkb",
    "ewkt",
    "ewkb",
    "twkb",
    "geojson",
    "ogc",
    "geometry",
    "geography",
    "spatial"
  ],
  "dependencies": {
    "@types/node": "*"
  }
}
